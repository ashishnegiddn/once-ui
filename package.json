{
  "name": "@once/ui",
  "version": "0.0.12",
  "license": "MIT",
  "repository": "https://github.com/scheduleonce/ui-components.git",
  "bugs": "https://github.com/scheduleonce/ui-components/issues",
  "author": "so-dinesh-rawat <dinesh.rawat@scheduleonce.com>",
  "typings": "index.d.ts",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "copy": "ncp dist/lib/ dist/",
    "update-paths": "node update-build-paths.ts",
    "build-npm": "rimraf dist && ng-packagr -p ng-package.json && npm run bundle-styles && npm run update-paths && npm run copy",
    "bundle-styles": "npm run bundle-styles-blue && npm run compile-scss-blue && npm run bundle-styles-green && npm run compile-scss-green",
    "bundle-styles-blue": "scss-bundle -e ./lib/themes/once-ui-theme-blue.scss -d ./dist/themes/once-ui-theme-blue.scss",
    "bundle-styles-green": "scss-bundle -e ./lib/themes/once-ui-theme-green.scss -d ./dist/themes/once-ui-theme-green.scss",
    "compile-scss-blue": "node-sass --include-path scss ./dist/themes/once-ui-theme-blue.scss ./dist/themes/once-ui-theme-blue.css && rm -f ./dist/themes/once-ui-theme-blue.scss",
    "compile-scss-green": "node-sass --include-path scss ./dist/themes/once-ui-theme-green.scss ./dist/themes/once-ui-theme-green.css && rm -f ./dist/themes/once-ui-theme-green.scss",
    "publish": "npm run build-npm && npm publish dist",
    "storybook": "start-storybook -s ./assets -p 9001 .storybook",
    "build": "build-storybook -s ./assets",
    "start": "node app.ts"
  },
  "keywords": [
    "Angular",
    "Once UI",
    "Shared UI library",
    "components",
    "NPM modules"
  ],
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/material": "^5.2.5",
    "@angular/material-moment-adapter": "^5.2.5",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@babel/core": "^7.0.0-beta.47",
    "@storybook/addon-actions": "^3.4.0-alpha.8",
    "@storybook/addon-knobs": "^3.4.3",
    "@storybook/addon-options": "^3.4.4",
    "@storybook/addons": "^3.4.0-alpha.8",
    "@storybook/angular": "^3.4.4",
    "babel-core": "^6.26.3",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.5",
    "glob": "^7.1.2",
    "json-parse-better-errors": "^1.0.2",
    "moment": "^2.19.3",
    "ncp": "^2.0.0",
    "ngx-perfect-scrollbar": "^5.3.5",
    "rxjs": "^5.5.6",
    "storybook-readme": "^3.2.0",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^9.3.0",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "ng-packagr": "^2.4.4",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "scss-bundle": "^2.3.2"
  }
}
